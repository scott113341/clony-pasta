{
  "name": "clony-pasta",
  "version": "0.3.1",
  "description": "Foolishly simple scaffolding tool using the ez template language.",
  "author": "Scott Hardy <scott.the.hardy@gmail.com> (https://www.scotthardy.me)",
  "license": "MIT",
  "repository": "https://github.com/scott113341/clony-pasta.git",
  "homepage": "https://github.com/scott113341/clony-pasta",
  "bugs": "https://github.com/scott113341/clony-pasta/issues",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "bin": "lib/cli.js",
  "preferGlobal": true,
  "scripts": {
    "clean": "rimraf lib/ && mkdirp lib/",
    "precompile": "npm run clean",
    "compile": "babel -d lib/ src/",
    "preversion": "npm run test && npm run compile",
    "test": "npm run compile && babel-node test/index.js"
  },
  "dependencies": {
    "eztl": "0.1.0",
    "minimist": "1.2.0",
    "readline-sync": "1.4.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "mkdirp": "0.5.1",
    "rimraf": "2.5.2",
    "tape": "4.5.1"
  }
}
